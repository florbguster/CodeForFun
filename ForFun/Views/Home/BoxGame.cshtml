
@{
    ViewBag.Title = "Spinnin' Boxes";
}
@Styles.Render( "~/boxGame/style" )


<div>Objective: Try to change all worried boxes into smileys. Without a mistake!</div> <br />
<section id="BoxGameSection">

    <div id="allDivs">
        @for (int i = 0; i < 72; i++)
        {
            <div class="home">
                <div class="happy"> </div>
            </div>
        }
    </div>


    @section scripts
    {
        <script>
            $('#CarRegistry').removeClass('active');
            $('#Game').addClass('active');
        </script>
    }
</section>
@Scripts.Render( "~/boxGame/js" )

